{
  "name": "ptp-responsive",
  "version": "1.0.0",
  "description": "Responsive css for themes on PTP",
  "main": "main.css",
  "scripts": {
    "sass:watch": "node-sass -w ./src -o ./dist",
    "sass:debug": "node-sass ./src -o ./dist",
    "sass:build": "node-sass --output-style compressed ./src -o ./dist",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint src/**/*.scss"
    "stylelint:fix": "stylelint src/**/*.scss --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Starblaster100/ptp-responsive.git"
  },
  "keywords": [
    "responsive",
    "css",
    "ptp"
  ],
  "author": "Starblaster100",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Starblaster100/ptp-responsive/issues"
  },
  "homepage": "https://github.com/Starblaster100/ptp-responsive#readme",
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "webpack-dev-server": "^3.11.2"
  }
}
